@model StorageFinalProject.Models.SaleForm
@{
    ViewBag.Title = "Add";
}

<h2>Add</h2>

@using (Html.BeginForm("Add", "ShopOperator"))
{
    @Html.ValidationSummary(true, "ოპერაცია ვერ განხორციელდა, შეამოწმეთ მოანცემები.");
    <div>
        <fieldset>
            <legend>Login</legend>
            <div class="editor-label">
                @Html.LabelFor(s => s.Category)
            </div>
             <div class="editor-field">
                @Html.TextBoxFor(u => u.UserName)
                @Html.ValidationMessageFor(u => u.UserName)
            </div>
        </fieldset>
    </div>
}